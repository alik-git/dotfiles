{
    "window.zoomLevel": 1,
    "python.pythonPath": "/home/kuwajerw/anaconda3/envs/pyt/bin/python",
    "editor.minimap.showSlider": "always",
    "vim.overrideCopy": true,
    "multiCommand.commands": [

        {
          "command": "multiCommand.printVariable",
     
          "sequence": [
            "toggleVim",
            "editor.action.clipboardCopyAction",
            "editor.action.insertLineAfter",
            {
              "command": "type",
              "args": {
                "text": "print(\""
              }
            },
            "editor.action.clipboardPasteAction",
            {
              "command": "type",
              "args": {
                "text": ": \" + str("
              }
            },
            "editor.action.clipboardPasteAction",
            {
                "command": "type",
                "args": {
                  "text": "))"
                }
            },
            "toggleVim",
          ]
        }
    ],
    // "workbench.iconTheme": "vscode-icons",
    "vsicons.presets.jsOfficial": true,
    "vsicons.presets.foldersAllDefaultIcon": true,
    "vim.useCtrlKeys": false,
    "vim.insertModeKeyBindings": [
      {
          "before": ["j", "j"],
          "after": ["<Esc>"]
      },
      {
          "before": ["f", "f"],
          "after":["p", "r", "i", "n", "t", "("]
      }
    ],
    "vim.normalModeKeyBindings": [
      {
          "before": ["f", "f"],
          "commands": ["editor.action.commentLine"]
      },
      {
        "before": [";"],
        "after": [":"]
      }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
      {
          "before": [
              ">"
          ],
          "commands": [
              "editor.action.indentLines"
          ]
      },
      {
          "before": [
              "<"
          ],
          "commands": [
              "editor.action.outdentLines"
          ]
      },
    ],
  "git.enableSmartCommit": true,
  "sync.gist": "593c280dbe75266c126ee7a7a216402c",
  "python.linting.pylintArgs": [
    "--errors-only",
    "--generated-members=numpy.* ,torch.* ,cv2.* , cv.*"
    ],
  "bracket-pair-colorizer-2.showHorizontalScopeLine": false,
  "bracket-pair-colorizer-2.showVerticalScopeLine": true,
  "bracket-pair-colorizer-2.colors": [
      "DodgerBlue ",
      "Chartreuse ",
      "HotPink ",
      "Cornsilk ",
      "DarkSalmon" 
  ],
  "workbench.activityBar.visible": true,
  "workbench.colorTheme": "Monokai"
}